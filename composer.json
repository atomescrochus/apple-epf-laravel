{
    "name": "appwapp/apple-epf-laravel",
    "description": "Provides models and other tools to use Apple's EPF in Laravel.",
    "keywords": [
        "atomescrochus",
        "apple-epf-laravel",
        "itunes",
        "epf"
    ],
    "homepage": "https://github.com/appwapp/apple-epf-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Andrew Landles",
            "email": "malandles@appwapp.com",
            "homepage": "https://www.appwapp.com",
            "role": "Lead Developer"
        },
        {
            "name": "Jean-Philippe Murray",
            "email": "jp@atomescroch.us",
            "homepage": "https://atomescroch.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/dom-crawler": "^3.2",
        "symfony/process": "^3.2",
        "guzzlehttp/guzzle": "~6.0",
        "nesbot/carbon": "~1.20",
        "alchemy/zippy": " ~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Appwapp\\EPF\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Appwapp\\EPF\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Appwapp\\EPF\\EPFServiceProvider"
            ]
        }
    }
}
